    "build": "yarn ci && yarn c",
    "build-dev": "yarn lint && yarn c",
    "prepack": "yarn build",
    "test": "yarn jest",
    "update": "rm -rf node_modules && yarn",
    "dev": "node lib/run.js"
  },
